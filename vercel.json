{
  "version": 2,
  "outputDirectory": ".next",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(?:[^/]+/)*[^/]+\\.(?:js|css|png|jpg|jpeg|gif|svg|ico|webp|avif|json|woff|woff2|ttf|eot|otf)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "dest": "/_next/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}